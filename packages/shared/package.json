{
  "name": "@interview-library/shared",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./i18n": {
      "import": "./dist/i18n/index.js",
      "require": "./dist/i18n/index.cjs",
      "types": "./dist/i18n/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/i18n/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/i18n/index.ts --format cjs,esm --dts --watch"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
